{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "module": "ESNext",
    "target": "ESNext",
    "esModuleInterop": true,
    "moduleResolution": "Node",
    "verbatimModuleSyntax": false,
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": "./src/",
    "paths": {
      "@components/*": ["components/*"],
      "@public/*": ["public/*"],
      "@utility/*": ["utility/*"],
      "@hooks/*": ["hooks/*"],
      "@layouts/*": ["layouts/*"],
      "@sections/*": ["sections/*"],
      "@types/*": ["types/*"],
      "@utils/*": ["utils/*"],
      "@context/*": ["context/*"]
    }
  }
}